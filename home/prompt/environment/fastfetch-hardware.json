{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/master/doc/json_schema.json",
  "logo": {
    "type": "none"
  },
  "modules": [
    "os",
    "host",
    "kernel",
    "shell",
    "display",
    "wm",
    "terminal",
    "cpu",
    "gpu",
    {
      "type": "memory",
      "format": "{total}"
    },
    {
      "type": "swap",
      "format": "{total}"
    },
    {
      "type": "disk",
      "showSubvolumes": false
    },
    "locale"
  ]
}
